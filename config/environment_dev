#!/usr/bin/env bash
set -a
# hardcoded or derived variables
PROJECT_ROOT="$( cd "$( dirname "${BASH_SOURCE[0]}" )"/.. && pwd )"
KUBECONFIG=$PROJECT_ROOT/config/cluster_secrets/kubeconfig_dev
AWS_PROFILE=hca
TF_VAR_aws_profile=${AWS_PROFILE}
TF_VAR_account_id=$(aws sts get-caller-identity --profile ${TF_VAR_aws_profile} | jq -r .Account)
TF_VAR_aws_region=us-east-1
TF_VAR_deployment_stage=dev
TF_VAR_terraform_bucket=org-humancellatlas-${TF_VAR_account_id}-terraform
TF_VAR_terraform_key=ingest/eks-dev.tfstate
TF_VAR_vpc_cidr_block=10.10.0.0/16
TF_VAR_subnet_cidr_block_one=10.10.1.0/24
TF_VAR_subnet_cidr_block_two=10.10.2.0/24
set +a
